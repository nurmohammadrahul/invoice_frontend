{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      }
    ],
    "env": {
      "REACT_APP_API_URL": "https://invoice-backend-eight-rho.vercel.app/api",
      "REACT_APP_NAME": "Invoice Management System",
      "REACT_APP_VERSION": "1.0.0"
    }
  }